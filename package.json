{
  "name": "fortcake-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "start": "next start",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "build": "next build && next export -o build",
    "build:ssr": "CI=false && GENERATE_SOURCEMAP=false next build",
    "integration-test": "start-server-and-test 'serve -s build -p 3000' http://localhost:3000 'cypress run'",
    "lint": "eslint './src/**/*.{js,jsx,ts,tsx}'",
    "next:lint": "next lint",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx}\"",
    "format:write": "prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "updateLPsAPR": "NODE_PATH=./src ts-node --compiler-options '{\"module\":\"commonjs\"}' scripts/updateLPsAPR.ts",
    "prepare": "husky install",
    "theme": "yarn upgrade fortcake-uikit-v2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@binance-chain/bsc-connector": "^1.0.0",
    "@ethersproject/experimental": "^5.5.0",
    "@next/bundle-analyzer": "^12.1.6",
    "@pancakeswap/sdk": "^2.4.0",
    "@reduxjs/toolkit": "^1.5.0",
    "@sentry/react": "^6.15.0",
    "@sentry/tracing": "^6.15.0",
    "@types/multicodec": "^1.0.0",
    "@types/node": "^13.13.5",
    "@types/qs": "^6.9.6",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.4",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-window": "^1.8.3",
    "@types/styled-components": "^5.1.7",
    "@uniswap/token-lists": "^1.0.0-beta.19",
    "@uniswap/v2-core": "^1.0.0",
    "@uniswap/v2-periphery": "^1.1.0-beta.0",
    "@web3-react/core": "^6.1.9",
    "@web3-react/injected-connector": "^6.0.7",
    "@web3-react/walletconnect-connector": "6.2.4",
    "@web3-react/walletlink-connector": "^6.2.13",
    "ajv": "^6.12.3",
    "axios": "^0.26.0",
    "bignumber.js": "^9.0.0",
    "canvas-confetti": "^1.3.3",
    "cids": "^1.0.0",
    "date-fns": "^2.21.3",
    "easymde": "^2.15.0",
    "ethers": "^5.5.2",
    "fast-json-stable-stringify": "^2.1.0",
    "fortcake-uikit-v2": "fortcake/fortcake-uikit-v2#nextjs",
    "graphql": "^15.5.0",
    "graphql-request": "^3.4.0",
    "js-cookie": "^2.2.1",
    "lightweight-charts": "^3.5.0",
    "localforage": "^1.10.0",
    "lodash": "^4.17.21",
    "multicodec": "^2.0.0",
    "multihashes": "^3.0.1",
    "next": "^12.1.6",
    "next-themes": "^0.2.0",
    "numeral": "^2.0.6",
    "qs": "^6.9.4",
    "react": "^17.0.1",
    "react-countup": "^4.3.3",
    "react-datepicker": "^4.2.1",
    "react-dom": "^17.0.1",
    "react-markdown": "^6.0.2",
